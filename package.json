{
  "name": "crichow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run clear-cache && next dev --turbopack",
    "build": "npm run clear-cache && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
